{
  "name": "instructure-ui",
  "version": "1.0.2",
  "description": "A UI component library made by Instructure Inc.",
  "author": "Instructure, Inc. Engineering and Product Design",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/instructure/instructure-ui.git"
  },
  "homepage": "http://instructure.github.io/instructure-ui/",
  "bugs": "https://github.com/instructure/instructure-ui/issues",
  "scripts": {
    "prestart": "npm run clean && npm install && npm run docs:min",
    "start": "http-server __build__/docs -p 8001",
    "start:watch": "cross-env NODE_ENV=development DEBUG=1 webpack-dev-server --hot --inline",
    "test": "npm run test:browser && npm run test:node",
    "test:browser": "cross-env NODE_ENV=test karma start --single-run --coverage",
    "test:lint": "eslint lib",
    "test:node": "npm run transpile && node __build__/lib/index.js",
    "test:watch": "cross-env NODE_ENV=test DEBUG=1 karma start",
    "generate": "./scripts/generate",
    "clean": "rimraf __build__ && rimraf .babel-cache",
    "docs": "cross-env NODE_ENV=development webpack",
    "docs:min": "cross-env NODE_ENV=production MINIFY=1 webpack",
    "dist": "cross-env NODE_ENV=production webpack --config webpack/dist.config.js",
    "dist:min": "cross-env MINIFY=1 NODE_ENV=production webpack --config webpack/dist.config.js",
    "transpile": "cross-env NODE_ENV=transpile BABEL_DISABLE_CACHE=1 babel lib --out-dir __build__/lib --ignore *.test.js",
    "build": "npm run clean && npm run dist && npm run dist:min && npm run transpile",
    "bump": "./scripts/bump",
    "bump:major": "./scripts/bump major",
    "bump:minor": "./scripts/bump minor",
    "bump:patch": "./scripts/bump patch",
    "pre-release": "./scripts/pre-release",
    "release": "./scripts/release",
    "release:beta": "./scripts/release -t beta",
    "release:rc": "./scripts/release -t rc",
    "deploy": "./scripts/deploy -d __build__/docs -r git@github.com:instructure/instructure-ui.git"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "6.7.0",
    "axe-core": "2.1.7",
    "babel-cli": "6.22.2",
