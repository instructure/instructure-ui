<%= DEPENDENCIES %>
  },
  "devDependencies": {
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "rimraf": "^3.0.0",
    "eslint": "^7",
<%= DEV_DEPENDENCIES %>
  }
}
