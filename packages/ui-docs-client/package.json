    "react-dom": "15.6.1"
  },
  "dependencies": {
    "@instructure/ui-core": "^3.4.0",
    "@instructure/ui-themeable": "^3.4.0",
    "@instructure/ui-utils": "^3.4.0",
    "babel-standalone": "^6.26.0",
    "classnames": "^2.2.5",
    "codemirror": "^5.27.4",
    "gray-matter": "^3.0.2",
