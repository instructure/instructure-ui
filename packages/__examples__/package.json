{
  "name": "docs-examples",
  "description": "UI libraries made by Instructure Inc.",
  "private": true,
  "version": "8.30.0",
  "author": "Instructure, Inc. Engineering and Product Design",
  "repository": {
    "type": "git",
    "url": "https://github.com/instructure/instructure-ui.git"
  },
  "homepage": "https://instructure.github.io/instructure-ui/",
  "bugs": "https://github.com/instructure/instructure-ui/issues",
  "scripts": {
    "prestart": "yarn bundle",
    "start": "ui-scripts --server -p 9001",
    "start:watch": "yarn build:examples && run -T ui-build --examples --watch -p 9090",
    "test:vrt": "run -T ui-test --vrt -p 9001",
    "accept:vrt": "run -T ui-test --vrt -p 9001 --auto-accept-changes",
    "bundle": "yarn build:examples && run -T ui-build --examples",
    "clean": "ui-scripts --clean",
    "generate:sketch": "story2sketch --url http://localhost:9001/iframe.html --output stories.asketch.json",
    "build:storybook": "run -T ui-build --examples",
    "build:examples": "node buildScripts/build-examples-json.js"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@instructure/canvas-theme": "8.30.0",
    "@instructure/command-utils": "8.30.0",
    "@instructure/ui-babel-preset": "8.30.0",
    "@instructure/ui-buttons": "8.30.0",
    "@instructure/ui-i18n": "8.30.0",
    "@instructure/ui-icons": "8.30.0",
    "@instructure/ui-tooltip": "8.30.0",
    "@instructure/ui-view": "8.30.0",
    "@instructure/ui-webpack-config": "8.30.0",
    "@instructure/uid": "8.30.0",
    "@storybook/addons": "^6",
    "@storybook/react": "^6",
    "@storybook/theming": "^6",
    "chromatic": "^6",
    "react": "^18",
    "react-dom": "^18",
    "story2sketch": "^1",
    "webpack-merge": "^5"
  },
  "devDependencies": {
    "@instructure/ui-test-utils": "8.30.0",
    "@storybook/builder-webpack5": "^6",
    "@storybook/manager-webpack5": "^6",
    "globby": "^11",
    "prop-types": "^15",
    "react-docgen": "^6.0.0-alpha.0",
    "webpack": "^5"
  },
  "resolutions": {
    "@storybook/react/webpack": "^5"
  }
}
