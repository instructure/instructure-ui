{
  "name": "docs-app",
  "private": true,
  "version": "8.28.2",
  "description": "Documentation and showcase application for Instructure-UI.",
  "author": "Instructure, Inc. Engineering and Product Design",
  "repository": {
    "type": "git",
    "url": "https://github.com/instructure/instructure-ui.git"
  },
  "homepage": "https://instructure.design",
  "bugs": "https://github.com/instructure/instructure-ui/issues",
  "scripts": {
    "prestart": "yarn build:scripts:ts && node -e 'require(\"./lib/build-docs.js\").buildDocs()'",
    "start": "run -T ui-build --bundle && ui-scripts --server -p 8001",
    "start:watch": "yarn prestart && run -T ui-build --bundle --watch -p 8080",
    "bundle": "yarn prestart && run -T ui-build --bundle",
    "lint": "run -T ui-test --lint",
    "lint:fix": "run -T ui-test --lint --fix",
    "build:scripts:ts": "run -T tsc -b tsconfig.node.build.json",
    "clean": "run -T ui-build --clean",
    "createStatsFile": "mkdirp ./__build__ && webpack --profile --json > __build__/bundleStats.json",
    "analyseStatsFile": "webpack-bundle-analyzer __build__/bundleStats.json"
  },
  "license": "MIT",
