    "pretty-format": "^24.9.0",
    "sizzle": "^2.3.4"
  },
  "devDependencies": {
    "@instructure/ui-babel-preset": "8.6.0",
    "@types/sizzle": "2.3.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
